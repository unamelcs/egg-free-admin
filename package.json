{
  "name": "free-admin",
  "version": "1.0.0",
  "description": "free admin egg framework",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-view-vue-ssr": "^3.3.1",
    "vue": "^2.6.11",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "easywebpack-vue": "^4.5.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-webpack": "^4.5.2",
    "egg-webpack-vue": "^2.0.2",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-free-admin",
